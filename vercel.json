{
  "version": 2,
  "builds": [
    {
      "src": "/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/.*",
      "dest": "node_modules/swagger-ui-dist/index.html"
    },
    {
      "src": "/api-docs/swagger-ui-bundle.js",
      "dest": "node_modules/swagger-ui-dist/swagger-ui-bundle.js"
    },
    {
      "src": "/api-docs/swagger-ui.css",
      "dest": "node_modules/swagger-ui-dist/swagger-ui.css"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}